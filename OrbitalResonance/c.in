sName		c			# Body's name
saModules 	spinbody          # Modules

bUseOrbParams	1

# Physical Properties
dMass		-2.5  		# Mass, negative -> Earth masses
dRadius	-1.367  		# Radius, negative -> Earth radii

# Orbital Properties
dEcc            1.0e-5		# Eccentricity
dSemi           0.0399          # Semi-major axis, negative -> AU
dInc            89.287             # Orbital inclination (degrees)
dLongA          0.0             # Longitude of the Ascending node (degrees)
dLongP          288.0             # Longitude of the Periapse (degrees)
dMeanA		0.0		# Mean Anomaly (degrees)
#sOrbitModel     rd4             # Use 4th Order disturbing function model

#Output
saOutputOrder Time LongP MeanLongitude -TotEn -TotAngMom SemimajorAxis -OrbPeriod Eccentricity
